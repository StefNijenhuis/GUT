<p id="notice"><%= notice %></p>

<div id="article">
	<div id="header">
		<h1>
		  <%= @association_test.title %>
		</h1>

		<p>
		  <%= @association_test.description %>
		</p>
	</div>

	<p>
	<% @association_attachments.in_groups_of(2).each do |group| %>
	  <div class="row">
	  	<% group.each do |p| %>
	  		<% if p	%>
	  			<div class="image_frame">
	  				<%= image_tag p.image_url %>
	  			</div>
	  		<% end %>
	  	<% end %>
	  </div>
	<% end %>
	</p>

	<%= link_to 'Edit', edit_association_test_path(@association_test) %> |
	<%= link_to 'Back', association_tests_path %>
<div>