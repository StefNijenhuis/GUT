<%= javascript_include_tag "s7.addthis.com/js/300/addthis_widget.js#pubid=ra-53579f7224643a4a" %>
<%= javascript_include_tag "vague.js" %>
<%= javascript_include_tag "timeCircles.js" %>
<%= javascript_include_tag "blurTest.js" %>


<% if @test.methodname === 1 %>
	<%= render 'memory' %>
<% elsif @test.methodname === 2 %>
    <%= render 'blur' %>
<% end %>

<h1><%=  @test.title %> </h1>
<h2>Testmethode: <%= @methodname %> </h2>
<h1><%=  @test.start_date.to_formatted_s(:month_and_day_and_year) %> </h1>

<h1><%=  @test.end_date.to_formatted_s(:month_and_day_and_year) %> </h1>
<p>
    <%=raw @test.introtext %> 
</p>
<p>
    <%=raw @test.outrotext %> 
</p>

<div class="addthis_toolbox addthis_default_style ">
<a class="addthis_button_preferred_1"></a>
<a class="addthis_button_preferred_2"></a>
<a class="addthis_button_preferred_3"></a>
<a class="addthis_button_preferred_4"></a>
<a class="addthis_button_compact"></a>
<a class="addthis_counter addthis_bubble_style"></a>
</div>

<% if current_user.id == @test.user_id %>
	<%=	link_to "bewerken", edit_usertest_path(@test) %>
<% end %>
<%= link_to "Terug naar overzicht", usertests_path() %>