<% if !current_user %>
	<% if @test.method_id === 1 %>
		<%= render 'memory' %>
	<% elsif @test.method_id === 2 %>
	    <%= render 'blur' %>
    <% elsif @test.method_id === 3 %>
	    <%= render 'grayscale' %>
	<% elsif @test.method_id === 4 %>
	   <%= render 'abtest' %>
	<% elsif @test.method_id === 5 %>
	   <%= render 'glyphtest' %>
	<% end %>
<% end %>

