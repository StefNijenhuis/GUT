<% if !current_user %>
	<% if @test.method_id === 1 %>
		<%= render 'memory' %>
	<% elsif @test.method_id === 2 %>
	    <%= render 'blur' %>
    <% elsif @test.method_id === 3 %>
	    <%= render 'grayscale' %>
	<% elsif @test.method_id === 4 %>
	   <%= render 'abtest' %>
	<% elsif @test.method_id === 5 %>
	   <%= render 'glyphtest' %>
	<% end %>
<% else %>

<table dir="ltr" width="500" border="1" summary="purpose/structure for speech output">
	<caption>Resultaten van de <% @test.title %></caption>
	<thead>
		<tr>
			<td>Nummer</td>
			<td>Gedaan op</td>
			<td>Browser</td>
			<td>IP</td>
			<td>Resolutie</td>
		</tr>
	</thead>
	<tbody>
		 <% @results.each do |r| %>
			
			<% if r.testperson %>
				<tr>
					<td><%= r.testperson.id %></td>
				<td><%= r.testperson.created_at.strftime("Test gedaan op  %m/%d/%Y om %I:%M%p") %></td>
				 	<td><%= r.testperson.browser %></td>
					<td><%= r.testperson.ip %></td>
				 	<td><%= r.testperson.width %> x <%= r.testperson.height %>
					 		<%= r.result["q1"] %>
							<%= r.result["q2"] %>
					</td>
				</tr>
			<% end %>
		 <% end %>
	</tbody>
</table>




<% end %>

